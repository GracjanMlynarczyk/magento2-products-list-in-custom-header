        <referenceBlock name="ghratzoo_header.links">
            <block class="Ghratzoo\ProductsList\Block\Link" name="add-new-header-link">
                <arguments>
                    <argument name="label" xsi:type="string" translate="true">Products List</argument>
                    <argument name="path" path="productslist/product/index" xsi:type="url"/>
